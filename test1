#include "linux/kernel.h"
#include <string.h>
#include <stdio.h>
#include <unistd.h>
#include "arch/x86/include/generated/uapi/asm/unistd_64.h"

int main(int argc, char** argv) {
  char *in = "This is a stupid message.";
  char msg_small[1];
  char msg[50]
  buffer_NULL = NULL;
  int output;

  /* Send a message containing 'in' */
  syscall(__NR_dm510_msgbox_put,in,strlen(in)+1);

  /* Read a message */
  output = (syscall(__NR_dm510_msgbox_get, buffer_NULL, 50)
  printf("NULL buffer outputs error code %d\n", output)

  output = (syscall(__NR_dm510_msgbox_get, msg_small, 50)
  printf("a too small buffer outputs error code %d\n", output)
  
  (syscall(__NR_dm510_msgbox_get, msg, 50)
  return 0;
}
