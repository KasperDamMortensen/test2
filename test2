#include "linux/kernel.h"
#include <string.h>
#include <stdio.h>
#include <unistd.h>
#include "arch/x86/include/generated/uapi/asm/unistd_64.h"

int main(int argc, char** argv) {
  char *in = "one.";
  char *in2 = "two.";
  char *in3 = "three.";
  char msg[50];
  int msglen;


  printf("tests order of output from the stack\n");
  print("The elements are put on the stack in the order:")

  print("%c\n",*in);
  print("%c\n",*in2);
  print("%c\n",*in3);

  syscall(__NR_dm510_msgbox_put,in,strlen(in)+1);

  syscall(__NR_dm510_msgbox_put,in2,strlen(in)+1);

  syscall(__NR_dm510_msgbox_put,in3,strlen(in)+1);

  print("The elements are removed from the stack in the order:");
  /* Read a message */
  syscall(__NR_dm510_msgbox_get, msg, 50);
  printf("%c\n",msg);

  syscall(__NR_dm510_msgbox_get, msg, 50);
  printf("%c\n",msg);

  syscall(__NR_dm510_msgbox_get, msg, 50);
  printf("%c\n",msg);
  return 0;
}
